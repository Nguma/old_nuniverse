<dl id="breadcrumbs">
  <dt class = "home" style="z-index:9999">
    <%= link_to(image_tag("/images/icons/icon_nuniverse.png", :alt => "nuniverse.net"),"/") %>
  </dt>
  <% path.taggings.each_with_index do |crumb, i| -%>
    <% #raise crumb.crumbs.inspect if i == 1%>
    <dd  style="z-index:<%= (999 - i).to_s %>;left:-<%= 30 * (i+1) %>px">
      <%= link_to_tagging(crumb, :max => 60, :class => "nuni #{crumb.last_tag.kind}_kind") %>
     
    </dd>
  <% end -%>
</dl>