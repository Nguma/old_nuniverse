 <%# geolocate([tag])%>
<% content_for :head do -%>
  <%#= GMap.header (:local_search => true, :large_map => true, :with_vml => false) %>
<% end -%>

<div class="nuniverse" id="nuniverse">
  <%= render :partial => "/nuniverse/breadcrumbs", :locals => 
  {
    :path => path
  }%>
  <div class="scroller"> 
    <div class="body">  
      <% page_for(:tag => tag, :path => path) do -%> 
        <%= content_for_page(:tag => tag, :path => path, :perspective => perspective, :filter => filter) %>
      <% end -%>
    </div>
  </div>
</div>

<div class="hidden" id="gmap_hidden_div">
  <%#= @map.div(:width => "100%", :height => 450) %>
</div>

<div class="hidden" id="content_template">
  <div class="content">
    <dl class="connections">
    </dl>
  </div>
</div>
