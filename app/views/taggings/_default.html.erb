<%= title_for(tagging.object, :service => @service)%>
<%= kind_options(@tagging) %>
<div class="delimiter">&nbsp;</div>
<div id="right_column">
  <%= render :partial => "map", :locals => {:tagging => @tagging} %>
  <%= render :partial => "contributors", :locals => {:contributors => @contributors}%>
  <%= render :partial => "ads" %>
</div>
<div id="left_column">
  <div class="box">
  <%= elements_proper_to_kind(@tagging) %>
  <div class="options">
   
    <%= link_to "Google this topic", "/google/#{@tagging.id}" , :class => "option" %>
    <%= link_to "Locate", "/map/#{@tagging.id}" , :class => "option" %>
    <%= link_to "Images", "/taggings/#{@tagging.id}?service=images" , :class => "option" %>
    <%= link_to 'Delete', @tagging, :confirm => 'Are you sure?', :method => :delete, :class => "option" %>
  </div>
  <%= render :partial => "/nuniverse/connections", :locals => {
    :service => @service,
    :items => @items,
    :selected => @selected,
    :order => @order,
    :tagging => @tagging
  }%>
  </div>
</div>
<% unless @service -%>
  <%= render :partial => "/taggings/find_or_create" , :locals => {
    :subject => tagging.object, 
    :path => tagging.full_path
   }%>
  <%#= link_to 'Edit', edit_list_path(@tag) %> 
  
<% else -%>
  <div class="options">
    <%= link_to "back to content", @tagging , :class => "option" %>
  </div>
<% end -%>