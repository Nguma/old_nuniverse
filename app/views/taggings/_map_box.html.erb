<% content_for :head do -%>
   <%= GMap.header (:small_zoom => true, :with_vml => false) %>
   <%= map.to_html %>
<% end -%>
<div class="box" id="map_box">
  <h2><span class="size">1</span> Address <span class="prop"><%= source.object.property('address')%></span><%= expander_icon %></h2>
  <div class="content">
    <%= @map.div(:width => "100%", :height => 220) %>
  </div>
  <div class="actions">
    <%= command(:label => "Change Address", :command => "new address")%>
  </div>
</div>
