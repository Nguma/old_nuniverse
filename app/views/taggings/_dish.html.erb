<div class="column" id="nav">
  <%= render :partial => "image", :locals => {:object => @tagging.object }%>
  <%= ad_box %>
  <%#= render :partial => "contributors", :locals => {:tagging => @tagging}%>
  <%= tag_box(:source => @tagging.object)%>
</div>
<div id="content">
  <div class="column medium" >

    <%= lists_for(@tagging)%>
  </div>
  <div class="column medium">
    <%= render :partial => "comments", :locals => {:source => @tagging}%>
    <%= bookmarks_box(:source => @tagging.object)%>
  </div>
</div>
