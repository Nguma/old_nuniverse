<div class="box <%= dom_class %>" id="<%= dom_id %>" >
  <% unless title.blank?%>
    <%= thumbnail_tag(source.object) rescue ""%>
    <h2><%= link_to(title, source) rescue title.capitalize%></h2>
  <% end -%>
  
  <%= spinner %>
  <div class="items" >
    <%#= will_paginate(items, :params => {:kind => kind}) %>
    <%#= render :partial => "/taggings/instance", :collection => items %>
  </div>

</div>