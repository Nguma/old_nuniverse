<div id="right_column">
  <%= render :partial => "/tags/profile", :locals => {:tag=> @tagging.object} %>
  <%= render :partial => "image", :locals => {:tagging => @tagging }%>
  <%= render :partial => "options", :locals => {:tagging => @tagging}%>
  <%= render :partial => "contributors", :locals => {:tagging => @tagging}%>
</div>
<div id="left_column">
  <div class="box">
    <ul class="tabs"> 
      <li class="current"><%= link_to "Overview" %></li>
      <li><%= link_to "Bookmarks", "/taggings/#{@tagging.id}?filter=bookmark"  %></li>
      <li><%= link_to "Cast", "/taggings/#{@tagging.id}?filter=person" %></li>
      <li><%= link_to "Characters", "/taggings/#{@tagging.id}?filter=character" %></li>
      <li><%= link_to "Images", "/taggings/#{@tagging.id}?service=images"  %></li>
    </ul>
    <%= render :partial => "/nuniverse/connections", :locals => {
      :service => @service,
      :items => @items,
      :selected => @selected,
      :order => @order,
      :tagging => @tagging
    }%>
    
  </div>
  <%= render :partial => "ads" %>
</div>