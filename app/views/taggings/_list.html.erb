<div class="column" style="width:25%">
  <%= render :partial => "profile", :locals => {:tagging=> @tagging} %>
  <%= render :partial => "ads" %>
  <%= render :partial => "contributors", :locals => {:tagging => @tagging}%>
  <%= render :partial => "options", :locals => {:tagging => @tagging}%>
  
  <div class="box">
    <h2><%= link_to("See it in images", "/taggings/#{@tagging.id}?service=images") %></h2>
  </div>
  
  
  <%= render :partial => "image", :locals => {:object => tagging.object }%>
  <%#= render :partial => "locations", :locals => {:tagging => @tagging}%>

</div>
<div class="column" style="width:73%">
  <%= render :partial => "map", :locals => {:tagging => @tagging, :items => @tagging.connections(:kind => 'location', :mode => 'exact')} %>
  <%= box(:source => @tagging, :class => "expanded")%>
  <%= render :partial => "comments", :locals => {:source => @tagging}%>
</div>